{
  "version": 2,
  "builds": [
    {
      "src": "api/savoire.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/savoire.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}